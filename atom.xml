---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>iocast - iocast.github.io</title>
 <link href="http://www.atmos.org/atom.xml" rel="self"/>
 <link href="http://www.atmos.org"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://iocast.github.io</id>
 <author>
   <name>iocast</name>
   <email>iocast@me.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.atmos.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.atmos.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>

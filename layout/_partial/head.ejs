    <!-- Import CSS & jQuery -->
    <% if(theme.materialcdn.use === true) { %>
        <%- cssLsload(theme.materialcdn.base + '/css/material.min.css') %>
        <%- cssLsload(theme.materialcdn.base + '/css/style.min.css') %>
        <%- partial('_partial/config_css') %>
        <%- jsLsload(theme.materialcdn.base + '/js/jquery.min.js') %>
        <%- jsLsload(theme.materialcdn.base + '/js/queue.min.js') %>
    <%}else{%>
        <%- cssLsload('css/material.min') %>
        <%- cssLsload('css/style.min') %>
        <%- partial('_partial/config_css') %>
        <%- jsLsload('js/jquery.min') %>
        <%- jsLsload('js/queue.min') %>
    <%}%>

    <!-- Baidu Analytics -->
    <% if(theme.analytics.baidu_id) { %>
        <%- partial('_widget/analytics/baidu-analytics') %>
    <% } %>
    <!-- Google Analytics -->
    <% if(theme.analytics.google_id) { %>
        <%- partial('_widget/analytics/google-analytics') %>
    <% } %>
    <!-- CNZZ Analytics -->
    <% if(theme.analytics.google_id) { %>
        <%- partial('_widget/analytics/cnzz-analytics') %>
    <% } %>

    <!-- Custom Head -->
    <% if (site.data.head) { %>
        <% for (var i in site.data.head) { %>
            <%- site.data.head[i] %>
        <% } %>
    <% } %>
</head>
